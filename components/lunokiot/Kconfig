menu "lunokIoT"
    choice LUNOKIOT_DEVICE
        prompt "Device support"
        help
            select Select device to support 
        
        config LUNOKIOT_DEVICE_ESP32
            bool "ESP-32 based board"
            help
                Generic unspecified ESP32
        config LUNOKIOT_DEVICE_M5STACK_ATOM_LITE
            bool "M5Atom Lite"
            help
                Enable support for M5Atom lite device
    endchoice
endmenu
